@model PortfolioDataDto
@{
    ViewData["Title"] = "UpdatePortfolioData";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<div class="col-md-12">
    <div class="chart-box">
        <h4>PortfolioData Update Page</h4>
        <br />
        <form method="post">
            <input type="hidden" asp-for="Id" />
            <label>Name</label>
            <input type="text" class="form-control" asp-for="Name" />
            <br />
            <label>Url</label>
            <input type="text" class="form-control" asp-for="Url" />
            <br />
            <label>ImagePath</label>
            <input type="text" class="form-control" asp-for="ImagePath" />
            <br />
            <button class="btn btn-primary">Update PortfolioData</button>
        </form>
    </div>
</div>