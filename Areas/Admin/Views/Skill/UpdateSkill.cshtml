@model SkillDto

@{
    ViewData["Title"] = "UpdateSkill";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<div class="col-md-12">
    <div class="chart-box">
        <h4>Skill Update Page</h4>
        <br />
        <form method="post">
            <input type="hidden" asp-for="Id" />
            <label>Title</label>
            <input type="text" class="form-control" asp-for="Title" />
            <br />
            <label>Ratio</label>
            <input type="text" class="form-control" asp-for="Ratio" />
            <br />
            <button class="btn btn-primary">Update Skill</button>
        </form>
    </div>
</div>